endef
export RAT
export GOODBYE
SRCS = ft.c parse.c philo.c create_forks.c create_ids.c
OBJECTS = ${SRCS:.c=.o}
OBJDIR = obj
OBJECTFILES = $(addprefix $(OBJDIR)/, $(OBJECTS))
CC = cc
CFLAGS = -pthread -Wall -Wextra -Werror -g
$(OBJDIR)/%.o: %.c philo.h
	@mkdir -p $(OBJDIR)
	@$(CC) $(CFLAGS) -c $< -o $@

$(NAME): $(OBJECTFILES)
	@$(CC) $(CFLAGS)  $(OBJECTFILES) -o $(NAME)
	@printf "$$RAT"

all: $(NAME)

clean:
	@rm -rf $(OBJDIR)
	@printf "$$GOODBYE"

fclean: clean
	@rm -f $(NAME)

re: fclean all

.PHONY: all clean fclean re
